<h1><%= @member.name %>会員詳細</h1>

<table class="table table-borderless">
  <tr>
    <th>会員ID</th>
    <td><%= @member.id %></td>
  </tr>

  <tr>
    <th>名前</th>
    <td><%= @member.name %></td>
  </tr>

  <tr>
    <th>メールアドレス</th>
    <td><%= @member.email %></td>
  </tr>

  <tr>
    <th><%= link_to "編集する", edit_admin_member_path, class: 'btn btn-primary' %></th>
    <td></td>
  </tr>
</table>
