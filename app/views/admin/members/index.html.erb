<h1>会員一覧</h1>

<table class="table">
  <thead>
  <tr>
    <th>会員ID</th>
    <th>名前</th>
    <th>メールアドレス</th>
  </tr>
 </thead>
  <% @members.each do |member| %>
 <tbody>
   <tr>
     <th><%= member.id %></th>
     <th><%= link_to admin_member_path(member.id) do %>
         <%= member.name %>
         <% end %>
     </th>
     <th><%= member.email %></th>
     <th>退会ステータス</th>
      <% if member.is_valid %>
        <td>有効</td>
      <% else %>
        <td>退会済</td>
      <% end %>
   </tr>
 </tbody>
  <% end %>





















</table>