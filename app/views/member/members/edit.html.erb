<h1><%= @member.name %>の会員情報編集</h1>

<%= form_with model: @member, url: members_update_path do |f| %>

<table>
    <tr>
     <th><%= f.label "名前" %></th>
     <td><%= f.text_field :name %></td>
    </tr>

    <tr>
     <th><%= f.label "メールアドレス" %></th>
     <td><%= f.email_field :email %></td>
    </tr>

    <tr>
     <th></th>
     <td><%= f.submit "編集内容を保存", class: 'btn btn-info' %></td>
     <td><%= link_to "退会する", members_unsubscribe_path, class: 'btn btn-danger' %></td>
    </tr>
</table>

<% end %>

